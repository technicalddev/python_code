{
  "extends": [
    "tslint:recommended",
    "tslint-config-airbnb",
    "tslint-config-prettier"
  ],
  "rules": {
    "align": {
      "options": ["parameters", "statements"]
    },
    "ordered-imports": false,
    "object-literal-sort-keys": false,
    "no-empty": false,
    "member-access": false,
    "no-console": false,
    "increment-decrement": false,
    "no-bitwise": true,
    "no-increment-decrement": false
  }
}
